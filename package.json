{
  "name": "@all41-dev/ui-components.host",
  "version": "4.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib-debug && ng serve --port 4210",
    "build": "run-s build:lib && run-s build:main",
    "build:lib": "ng build --project components-ui --configuration production && npm run copy:npmrc",
    "build:lib-debug": "ng build --project components-ui",
    "build:main": "ng build",
    "test": "exit 0",
    "lint": "eslint src/** --fix",
    "e2e": "ng e2e",
    "copy:npmrc": "cp-cli ./.npmrc ./dist/components-ui/.npmrc",
    "publish:lib": "node ./update-lib-version.js && cd dist/components-ui && npm publish --access public",
    "version:alpha": "npm version prerelease --preid=alpha",
    "version:beta": "npm version prerelease --preid=beta",
    "preversion": "run-p test lint build:lib",
    "postversion": "git push && git push --tags && npm run publish:lib",
    "ncu:view": "ncu -x typescript",
    "ncu:upd": "ncu -x typescript -u && npm i"
  },
  "dependencies": {
    "@angular/animations": "^13.3.9",
    "@angular/common": "^13.3.9",
    "@angular/compiler": "^13.3.9",
    "@angular/core": "^13.3.9",
    "@angular/forms": "^13.3.9",
    "@angular/platform-browser": "^13.3.9",
    "@angular/platform-browser-dynamic": "^13.3.9",
    "@angular/router": "^13.3.9",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-oauth2-oidc-jwks": "^13.0.1",
    "core-js": "^3.7.0",
    "moment": "^2.29.2",
    "rxjs": "^6.6.3",
    "scroll-into-view-if-needed": "^2.2.29",
    "tabulator-tables": "^4.9.3",
    "zone.js": "^0.11.3"
  },
  "deps-disabled": {},
  "devDependencies": {
    "@all41-dev/ui-components": "file:dist/components-ui",
    "@angular-devkit/build-angular": "^13.3.6",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^13.3.6",
    "@angular/compiler-cli": "^13.3.9",
    "@angular/language-service": "^13.3.9",
    "@types/estree": "0.0.51",
    "@types/jasmine": "^3.6.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.7",
    "@types/tabulator-tables": "^4.9.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "codelyzer": "^6.0.1",
    "cp-cli": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-markdown": "^2.2.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^13.3.1",
    "npm-run-all": "^4.1.5",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "tsickle": "0.39.1",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "typescript": "~4.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
