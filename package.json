{
  "name": "@all41-dev/ui-components.host",
  "version": "2.0.12",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib-debug && ng serve --port 4210",
    "build": "run-s build:lib && run-s build:main",
    "build:lib": "ng build --project components-ui --prod && npm run copy:npmrc",
    "build:lib-debug": "ng build --project components-ui",
    "build:main": "ng build",
    "test": "exit 0",
    "lint": "eslint src/** --fix",
    "e2e": "ng e2e",
    "copy:npmrc": "cp-cli ./.npmrc ./dist/components-ui/.npmrc",
    "publish:lib": "node ./update-lib-version.js && cd dist/components-ui && npm publish --access public",
    "preversion": "run-p test lint build:lib",
    "postversion": "git push && git push --tags && npm run publish:lib",
    "ncu:view": "ncu -x typescript",
    "ncu:upd": "ncu -x typescript -u && npm i"
  },
  "dependencies": {
    "@angular/animations": "^10.0.1",
    "@angular/common": "^10.0.1",
    "@angular/compiler": "^10.0.1",
    "@angular/core": "^10.0.1",
    "@angular/forms": "^10.0.1",
    "@angular/platform-browser": "^10.0.1",
    "@angular/platform-browser-dynamic": "^10.0.1",
    "@angular/router": "^10.0.1",
    "angular-oauth2-oidc": "^9.0.0",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.4",
    "scroll-into-view-if-needed": "^2.2.24",
    "zone.js": "^0.10.3"
  },
  "deps-disabled": {},
  "devDependencies": {
    "@all41-dev/ui-components": "file:dist/components-ui",
    "@angular-devkit/build-angular": "^0.1000.0",
    "@angular-devkit/build-ng-packagr": "^0.1000.0",
    "@angular-devkit/build-optimizer": "^0.1000.0",
    "@angular/cli": "^10.0.0",
    "@angular/compiler-cli": "^10.0.1",
    "@angular/language-service": "^10.0.1",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "codelyzer": "^5.2.1",
    "cp-cli": "^2.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.3",
    "ts-node": "^8.6.2",
    "tsickle": "0.38.0",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "typescript": "^3.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
